<h2>Users Table</h2>
    <table class="table">
        <thead>
            <tr>
                <th>Username</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            <% users.forEach(user => { %>
                <tr>
                    <td>
                        <a href="<%= `/users/${user._id}` %>">
                            <%= user.username %>
                        </a>
                    </td>
                    <td><%= user.email %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
